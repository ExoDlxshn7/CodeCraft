@{
    ViewData["Title"] = "Profile Page";
}
<script src="https://kit.fontawesome.com/b090154bf0.js" crossorigin="anonymous"></script>
<body>
<div id="container-left">
    <img id="profile-picture" src="https://i.imgur.com/Z8k08or.png">

    <div id="navbar-left">
        <a id="navbar-item" href="">Friends</a><br>
        <a id="navbar-item" href="">Edit Profile</a><br>
        <a id="navbar-item" href="">Logout</a>
    </div>
</div>
<div id="container-right">
    
    <div id="container-post">
        <div id="post-pp">
            <img id="pp-content" src="https://i.imgur.com/Z8k08or.png">
        </div>
        <textarea id="post-textarea" placeholder="Write something"></textarea>
        <div id="post-icons">
            <i class="fa-regular fa-image"></i>
            <i class="fa-regular fa-face-smile"></i>
            <i class="fa-solid fa-location-dot"></i>
        </div>
        <div id="post" class="AlignRight">
            <button id="post-button">Post</button>
        </div>
    </div>


    <div id="container-post">
        <div id="post-header">
            <a href="">
                <div id="post-pp">
                    <img alt="profile picture" id="pp-content" src="https://i.imgur.com/Z8k08or.png">
                </div>    
                <div id="post-profile">
                test
                <br>
                test
                </div>
            </a>
        </div>
    <div id="post-contents">
        <p></p>
        <img id="left-50" src="https://i.imgur.com/ZB6R3A3.png">
        <img id="right-50" src="https://i.imgur.com/5viBDND.png">
    </div>
    <div id="post-interaction">
        <a href="">Like</a>
        <a href="">Comment</a>
        <a href="">Favorite</a>
        <a href="">Share</a>
    </div>
</div>
</body>